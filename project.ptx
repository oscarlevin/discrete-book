<?xml version="1.0" encoding="utf-8"?>
<!-- This file, the project manifest, provides the overall configuration for your PreTeXt project. To edit the content of your document, open `source/main.ptx`. See https://pretextbook.org/doc/guide/html/processing-CLI.html#cli-project-manifest. -->
<project ptx-version="2">
  <targets>
    <target 
      name="web" 
      format="html"
      source="dmoi.ptx" 
      publication="publication.ptx"
      xsl="dmoi-html.xsl"
    >
      <stringparams html.css.extra="external/custom-styles.css" />
    </target>

    <target
      name="runestone" 
      format="html" 
      platform="runestone"
      source="dmoi.ptx"
      publication="runestone.ptx"
      xsl="dmoi-html.xsl"
    >
      <stringparams html.css.extra="external/custom-styles.css" />
    </target>

    <target 
      name="print" 
      format="pdf"
      source="dmoi.ptx"
      publication="publication.ptx"
      xsl="dmoi-latex.xsl" 
    />
    <target 
      name="print-sample" 
      format="pdf"
      latex-engine="pdflatex"
      output-filename="dmoi-sample-levin.pdf"
      source="dmoi-sample.ptx"
      publication="publication.ptx"
      xsl="dmoi-latex.xsl" 
    />
    <target 
      name="crc-print" 
      format="pdf"
      latex-engine="pdflatex"
      output-filename="dmoi-sample-crc.pdf"
      source="dmoi-sample.ptx"
      publication="crc.ptx"
      xsl="dmoi-latex-crc.xsl"
    />
    <target 
      name="test-print" 
      format="pdf"
      latex-engine="pdflatex"
      source="dmoi-sample.ptx"
      publication="crc.ptx"
    />
    <target 
      name="latex"
      format="latex"
      source="dmoi.ptx"
      publication="publication.ptx"
      xsl="dmoi-latex.xsl"
    />
    <target 
      name="crc-latex"
      format="latex"
      source="dmoi-sample.ptx"
      publication="crc.ptx"
      xsl="dmoi-latex-crc.xsl"
    />
    <target 
      name="epub"
      format="epub"
      source="dmoi.ptx"
      publication="publication.ptx"
      xsl="dmoi-latex.xsl"
    />
  </targets>
</project>
