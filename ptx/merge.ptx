            </pg-macros>
        <setup><pg-code>
                Context("IntegerFunctions");
                $ans1 = Compute("C(20,4)*C(16,4)*C(12,4)*C(8,4)*C(4,4)");
                $ans2 = Compute("5!*C(15,3)*C(12,3)*C(9,3)*C(6,3)*C(3,3)");
