  <exercise permid="AUc">
    <webwork>
        <setup>
          <pg-code>
            Context("Interval");
          </pg-code>
        </setup>
        <statement>
          <p permid="NpL">
            Let <m>A = \{1, 4, 9\}</m> and <m>B = \{1, 3, 6, 10\}</m>.
            Find each of the following sets.

            <ol permid="sUe">
              <li permid="iuZ">
                <p permid="jON">
                  <m>A \cup B</m>.
                </p>

                <p permid="PVW">
                  <var name="Set(1,3,4,6,9,10)" width="15"/>
                </p>
              </li>

              <li permid="OCi">
                <p permid="wdf">
                  <m>A \cap B</m>.
                </p>

                <p permid="cko">
                  <var name="Set(1)" width="15"/>
                </p>
              </li>

              <li permid="uJr">
                <p permid="Irx">
                  <m>A \setminus B</m>.
                </p>

                <p permid="oyG">
                  <var name="Set(4,9)" width="15"/>
                </p>
              </li>

              <li permid="aQA">
