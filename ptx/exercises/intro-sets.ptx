  <exercise permid="BOR">
    <statement>
      <p permid="OkA">
        Draw a Venn diagram to represent each of the following:

        <ol permid="YgP">
          <li permid="Tbx"><m>A \cup \bar B</m></li>

          <li permid="ziG"><m>\bar{(A \cup B)}</m></li>

          <li permid="fpP"><m>A \cap (B \cup C)</m></li>

          <li permid="LwY"><m>(A \cap B) \cup C</m></li>

          <li permid="rEh"><m>\bar A \cap B \cap \bar C</m></li>

          <li permid="XLq"><m>(A \cup B) \setminus C</m></li>
        </ol>
      </p>
    </statement>
    <solution permid="Ftr">
      <p permid="DbB">
        <ol permid="uUm">
          <li permid="DSz">
            <p permid="uNi">
              <m>A \cup \bar B</m>:
            </p>

            <sidebyside permid="CUk" width="20%">

              <image permid="oCx" xml:id="A-or-not-B">

            <latex-image>
              \begin{tikzpicture}[fill=gray!50, scale=.5]
                  \fill \circleA;
                    \begin{scope}
                    \clip \circleB \twosetbox;
                    \fill \twosetbox;
                    \end{scope}
                    \draw[thick] \circleA \circleAlabel \circleB \circleBlabel \twosetbox;
                  \end{tikzpicture}
            </latex-image>
              </image>

            </sidebyside>

          </li>

          <li permid="jZI">
            <p permid="aUr">
              <m>\bar{(A \cup B)}</m>:
            </p>

            <sidebyside permid="jbt" width="20%">

              <image permid="UJG" xml:id="not-A-or-B">

                    <latex-image>
                      \begin{tikzpicture}[scale=.5, fill=gray!50]
          \fill \twosetbox;
          \fill[white] \circleA \circleB;
          \draw[thick] \circleA \circleAlabel \circleB \circleBlabel \twosetbox;
        \end{tikzpicture}
                    </latex-image>
              </image>

            </sidebyside>

          </li>

          <li permid="QgR">
            <p permid="HbA">
              <m>A \cap (B \cup C)</m>:
            </p>

            <sidebyside permid="PiC" width="20%">

              <image permid="AQP" xml:id="A-and-B-or-C">

                    <latex-image>
                      \begin{tikzpicture}[fill=gray!50, scale=.5]
        \begin{scope}
          \clip \circleA;
          \fill \circleB \circleC;
        \end{scope}
        \draw[thick] \circleA \circleAlabel \circleB \circleBlabel \circleC \circleClabel \threesetbox;
        \end{tikzpicture}
                    </latex-image>
              </image>

            </sidebyside>

          </li>

          <li permid="woa">
            <p permid="niJ">
              <m>(A \cap B) \cup C</m>:
            </p>

            <sidebyside permid="vpL" width="20%">

