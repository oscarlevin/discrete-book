  <exercise permid="VpJ">
    <webwork><statement>
      <p permid="NEg">
        Find <m>3 + 7 + 11+ \cdots + 427</m>.
      </p>

      <p permid="tLp">
        <var name="23005" width="5"/>
      </p>
    </statement>
    <solution permid="vHG">
      <p permid="zAB">
        <m>\frac{430\cdot 107}{2} = 23005</m>.
