  <exercise permid="BwS">
    <webwork>
    <setup>
    <pg-code>Context()->variables->are(n => "Real");
    $ans1 = Formula("n+2");
    $ans2 = Formula("4n+2");
    $ans3 = Formula("((4n+8)*(n+2))/2");</pg-code>
    </setup>
    <statement>
      <p permid="ZSy">
        Consider the sequence <m>2, 6, 10, 14, \ldots, 4n + 6</m>.

        <ol permid="GQF">
          <li permid="Qkr">
            <p permid="Gvg">
              How many terms are there in the sequence?
            </p>

            <p permid="mCp">
              <var name="$ans1" width="10"/>
            </p>
          </li>

          <li permid="wrA">
            <p permid="SJy">
              What is the second-to-last term?
            </p>

            <p permid="yQH">
              <var name="$ans2" width="10"/>
            </p>
          </li>

          <li permid="cyJ">
            <p permid="eXQ">
              Find the sum of all the terms in the sequence.
            </p>

            <p permid="LeZ">
              <var name="$ans3" width="10"/>
            </p>
          </li>
        </ol>
      </p>
    </statement>
    <solution permid="bOP">
      <p permid="fHK">
        <ol permid="JLl">
          <li permid="IFS"><m>n+2</m> terms.</li>

          <li permid="oNb"><m>4n+2</m>.</li>

          <li permid="UUk"><m>\frac{(4n+8)(n+2)}{2}</m>.</li>
