  <exercise permid="veQ">
    <statement>
      <p permid="zWa">
        Let <m>A = \{a, b, c, d\}</m>. Find <m>\pow(A)</m>. </p>
    </statement>
    <hint permid="Xma">
      <p permid="UUk">
        We are looking for a set containing 16 sets.
      </p>
    </hint>
    <solution permid="reR">
      <p permid="kdi">
        <md permid="anX">
          <mrow>\pow(A) = \{\amp
        \emptyset, \{a\}, \{b\}, \{c\}, \{d\}, \{a,b\}, \{a,c\}, \{a,d\}, \{b,c\}, \{b,d\},</mrow>
          <mrow>\amp
        \{c,d\} \{a,b,c\}, \{a,b,d\}, \{a,c,d\}, \{b,c,d\}, \{a,b,c,d\}\}</mrow>
        </md>. </p>
    </solution>
  </exercise>

